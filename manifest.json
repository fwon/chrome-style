{
  "manifest_version": 2,
  "background": { "scripts": ["background.js"] },
  "name": "WebPage Style",
  "description": "This extension change style of WebPage.",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "Set this page's color.",
    "default_icon": {
         "19": "icon2.png",
         "38": "icon.png"
    },
    "default_popup": "popup.html"
  }
}